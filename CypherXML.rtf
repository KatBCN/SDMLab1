\cb4 WITH [x in authors._authors WHERE x._type = "author"] AS individuals, p\
\pard\pardeftab720\li1146\fi-10\partightenfactor0
\cf2 \cb3 UNWIND individuals as individual\cb4 \
\pard\tx1137\pardeftab720\fi1146\partightenfactor0
\cf2 MERGE (a:Author \{name:individual._text\})\
MERGE (a)-[:WROTE]->(p);\
