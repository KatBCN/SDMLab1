\f2 \cf2 \cb4 WITH "https://raw.githubusercontent.com/KatBCN/SDMLab1/main/BigDataMiningAndAnalytics.xml" AS uri\cb1 \
\cb4 CALL apoc.load.xml(uri, '', \{\}, true)\cb1 \
\cb4 YIELD value\cb1 \
\cb4 UNWIND value._result as result\cb1 \
\cb4 UNWIND result._hits as hits \cb1 \
\cb4 WITH [x in hits._hit WHERE x._info] as articles\cb1 \
\cb4 UNWIND articles as article\cb1 \
\cb4 WITH [item in article._info WHERE item._type="title"][0]._text as title,\cb1 \
\cb4 [item in article._info WHERE item._type="venue"][0]._text as journal,\
[item in article._info WHERE item._type="volume"][0]._text as volume,\
[item in article._info WHERE item._type="number"][0]._text as number,\cb1 \
\cb4 ([item in article._info WHERE item._type="venue"][0]._text + \cb1 \
\cb4 ",vol." + [item in article._info WHERE item._type="volume"][0]._text + \cb1 \
\cb4 "-" + [item in article._info WHERE item._type="number"][0]._text) as journalVolume,\cb1 \
\cb4 [item in article._info WHERE item._type="pages"][0]._text as pages,\cb1 \
\cb4 [item in article._info WHERE item._type="year"][0]._text as year,\cb1 \
\cb4 [item in article._info WHERE item._type="type"][0]._text as type,\cb1 \
\cb4 [item in article._info WHERE item._type="access"][0]._text as access,\
[item in article._info WHERE item._type\cb3 ="ee"][0]._text as doiLink,\cb1 \
\pard\tx1137\pardeftab720\fi1146\partightenfactor0
\cf2 \cb4 [item in article._info WHERE item._type = "authors"] AS authorList\cb1 \
\cb4 \
MERGE (p:Paper \{title: title\})\cb1 \
\cb4 SET p.access = access, p.type = type, p.pages = pages, p.doiLink = doiLink\cb1 \
\
\cb4 MERGE (jv:JournalVolume \{title: journalVolume\})\cb1 \
\cb4 SET jv.year = year, jv.volume = volume, jv.number = number\cb1 \
\
\cb4 MERGE (j:Journal \{title: journal\})\cb1 \
\
\cb4 MERGE (p)-[:PUBLISHED_IN]->(jv)\cb1 \
\
\cb4 MERGE (jv)-[:VOLUME_OF]->(j)\cb1 \
\
\cb4 WITH p, authorList\cb1 \
